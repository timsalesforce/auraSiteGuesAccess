                        var _iteratorError2 = undefined;

                        try {
                          for (var _iterator2 = mutationRecord.addedNodes[Symbol.iterator](), _step2; !(_iteratorNormalCompletion2 = (_step2 = _iterator2.next()).done); _iteratorNormalCompletion2 = true) {
